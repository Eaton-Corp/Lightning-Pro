<Window x:Class="LightningPRO.PrintLabels"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LightningPRO"
        mc:Ignorable="d"
        Title="PrintLabels" Height="450" Width="800" MinHeight="450" MinWidth="800" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="230*"/>
            <RowDefinition Height="189*"/>
        </Grid.RowDefinitions>
        <Image x:Name="QRCodeImage" HorizontalAlignment="Left" Height="140" Margin="30,145,0,0" VerticalAlignment="Top" Width="165" Grid.RowSpan="2"/>
        <Image x:Name="CSALabel" HorizontalAlignment="Left" Height="155" Margin="210,140,0,0" VerticalAlignment="Top" Width="555" Grid.RowSpan="2"/>
        <Button Background="Blue" Foreground="White" Content="Print" HorizontalAlignment="Left" Margin="545,135,0,0" Grid.Row="1" VerticalAlignment="Top" Width="105" Height="30"/>
        <Label FontSize="24" Content="Print Label" HorizontalAlignment="Left" Margin="75,30,0,0" VerticalAlignment="Top" Height="45" Width="150"/>
    </Grid>
</Window>
