<Window x:Class="PRL123_Final.SR"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PRL123_Final"
        mc:Ignorable="d"
        Title="SR" Height="1092" Width="979" MinHeight="1092" MinWidth="979" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition Width="825"/>
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="975"/>
            <RowDefinition />
        </Grid.RowDefinitions>
        <Button Grid.Column="1" Grid.Row="0" Background="Blue" Foreground="White" Content="Print" HorizontalAlignment="Left" VerticalAlignment="Center" Width="75" Click="Print_Click"/>
        <Image Name="Report" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="Uniform" />
    </Grid>
</Window>
