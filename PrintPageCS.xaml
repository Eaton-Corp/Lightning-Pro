<Window x:Class="PRL123_Final.PrintPageCS"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PRL123_Final"
        mc:Ignorable="d"
        Title="PrintPageCS" Height="900" Width="600">
    <Grid>
        <Image x:Name="CSALabel" HorizontalAlignment="Left" Height="700"  VerticalAlignment="Top" Width="400"/>
        <Button Background="Blue" Foreground="White" Content="Print" HorizontalAlignment="Left"  VerticalAlignment="Top" Width="105" Height="30" Click="Click_Print" Margin="0,850,0,0"/>
    </Grid>
</Window>
