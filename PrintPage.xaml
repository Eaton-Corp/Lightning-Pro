<Window x:Class="LightningPRO.PrintPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LightningPRO"
        mc:Ignorable="d"
        Title="PrintPage" Height="230" Width="560" MinHeight="230" MinWidth="560" ResizeMode="NoResize" >
    <Grid>
        <Image x:Name="CSALabel" HorizontalAlignment="Left" Height="155"  VerticalAlignment="Top" Width="555"/>
        <Button Background="Blue" Foreground="White" Content="Print" HorizontalAlignment="Left"  VerticalAlignment="Top" Width="105" Height="30" Click="Click_Print" Margin="0,165,0,0"/>
    </Grid>
</Window>
